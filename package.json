{
  "name": "yeoman-generator",
  "version": "5.7.0",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "yeoman",
    "app"
  ],
  "homepage": "http://yeoman.io",
  "repository": "yeoman/generator",
  "license": "BSD-2-Clause",
  "author": "Yeoman",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./typed": {
      "types": "./dist/index.d.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "doc": "npm run doc:generate && npm run doc:fix && npm run doc:prettier",
    "doc:fix": "sed -i -e 's:^[[:space:]]*<!--[[:space:]]*$::g' -e 's:^[[:space:]]*-->[[:space:]]*$::g' $npm_package_config_doc_path$DOC_FOLDER/global.html || true",
    "doc:generate": "jsdoc -c jsdoc.json -d $npm_package_config_doc_path$DOC_FOLDER",
    "doc:prettier": "prettier $npm_package_config_doc_path$DOC_FOLDER --write --ignore-path .prettierignore-doc",
    "prepare": "npm run build",
    "pretest": "xo",
    "test": "c8 mocha --forbid-only"
  },
  "config": {
    "doc_path": "../yeoman-generator-doc/"
  },
  "dependencies": {
    "@types/node": "^16.18.3",
    "chalk": "^5.1.2",
    "dargs": "^8.1.0",
    "debug": "^4.1.1",
    "execa": "^6.1.0",
    "github-username": "^7.0.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.7",
    "prettier-plugin-packagejson": "^2.3.0",
    "read-pkg-up": "^9.1.0",
    "semver": "^7.2.1",
    "simple-git": "^3.15.0",
    "sort-keys": "^5.0.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "c8": "^7.12.0",
    "coveralls": "^3.1.1",
    "esmock": "^2.0.9",
    "inquirer": "^8.2.5",
    "jsdoc": "^4.0.2",
    "mem-fs": "^2.2.1",
    "mem-fs-editor": "^9.5.0",
    "mocha": "^10.1.0",
    "nock": "^13.2.9",
    "prettier": "^2.8.0",
    "sinon": "^15.0.0",
    "through2": "^4.0.2",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^4.9.3",
    "xo": "^0.53.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^3.12.1",
    "yeoman-test": "^7.0.0"
  },
  "peerDependencies": {
    "yeoman-environment": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "yeoman-environment": {
      "optional": true
    }
  },
  "engines": {
    "node": "^14.15.0 || ^16.13.0 || >=18.12.0"
  }
}
