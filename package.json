{
  "name": "yeoman-generator",
  "version": "7.1.1",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "yeoman",
    "app"
  ],
  "homepage": "http://yeoman.io",
  "repository": "yeoman/generator",
  "license": "BSD-2-Clause",
  "author": "Yeoman",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.js",
      "default": "./dist/index.js"
    },
    "./typed": {
      "types": "./dist/index.d.js",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-types",
    "copy-types": "cpy \"src/**/*.d.(|c|m)ts\" dist/",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "doc": "npm run doc:generate && npm run doc:fix && npm run doc:prettier",
    "doc:fix": "sed -i -e 's:^[[:space:]]*<!--[[:space:]]*$::g' -e 's:^[[:space:]]*-->[[:space:]]*$::g' $npm_package_config_doc_path$DOC_FOLDER/global.html || true",
    "doc:generate": "jsdoc -c jsdoc.json -d $npm_package_config_doc_path$DOC_FOLDER",
    "doc:prettier": "prettier $npm_package_config_doc_path$DOC_FOLDER --write --ignore-path .prettierignore-doc",
    "prepare": "npm run build",
    "pretest": "xo",
    "test": "c8 esmocha --forbid-only"
  },
  "config": {
    "doc_path": "../yeoman-generator-doc/"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.18.5",
    "@yeoman/namespace": "^1.0.0",
    "chalk": "^5.3.0",
    "debug": "^4.1.1",
    "execa": "^8.0.1",
    "github-username": "^7.0.0",
    "json-schema": "^0.4.0",
    "latest-version": "^7.0.0",
    "lodash-es": "^4.17.21",
    "mem-fs-editor": "^11.0.1",
    "minimist": "^1.2.8",
    "read-package-up": "^11.0.0",
    "semver": "^7.5.4",
    "simple-git": "^3.20.0",
    "sort-keys": "^5.0.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.9",
    "@types/ejs": "^3.1.4",
    "@types/inquirer": "^9.0.4",
    "@types/minimist": "^1.2.3",
    "@types/semver": "^7.5.3",
    "@types/sinon": "^17.0.1",
    "@types/text-table": "^0.2.3",
    "@yeoman/adapter": "^1.4.0",
    "@yeoman/transform": "^1.2.0",
    "c8": "^8.0.1",
    "cpy-cli": "^5.0.0",
    "ejs": "^3.1.9",
    "esmocha": "^1.0.1",
    "inquirer": "^9.2.11",
    "jsdoc": "^4.0.2",
    "nock": "^13.3.4",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "sinon": "^18.0.0",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^5.2.2",
    "xo": "^0.56.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^4.0.0",
    "yeoman-test": "^8.1.0"
  },
  "peerDependencies": {
    "@yeoman/types": "^1.1.1",
    "mem-fs": "^4.0.0",
    "yeoman-environment": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "yeoman-environment": {
      "optional": true
    }
  },
  "engines": {
    "node": "^18.17.0 || >=20.5.0"
  },
  "overrides": {
    "yeoman-test": {
      "yeoman-generator": "file:."
    }
  }
}
