{
  "name": "yeoman-generator",
  "version": "7.3.3",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "yeoman",
    "app"
  ],
  "homepage": "http://yeoman.io",
  "repository": "yeoman/generator",
  "license": "BSD-2-Clause",
  "author": "Yeoman",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.js",
      "default": "./dist/index.js"
    },
    "./typed": {
      "types": "./dist/index.d.js",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-types",
    "copy-types": "cpy \"src/**/*.d.(|c|m)ts\" dist/",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "doc": "npm run doc:generate && npm run doc:fix && npm run doc:prettier",
    "doc:fix": "sed -i -e 's:^[[:space:]]*<!--[[:space:]]*$::g' -e 's:^[[:space:]]*-->[[:space:]]*$::g' $npm_package_config_doc_path$DOC_FOLDER/global.html || true",
    "doc:generate": "jsdoc -c jsdoc.json -d $npm_package_config_doc_path$DOC_FOLDER",
    "doc:prettier": "prettier $npm_package_config_doc_path$DOC_FOLDER --write --ignore-path .prettierignore-doc",
    "prepare": "npm run build",
    "pretest": "eslint . && npm run build",
    "test": "vitest run --coverage"
  },
  "config": {
    "doc_path": "../yeoman-generator-doc/"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.9",
    "@types/node": ">=18.18.5",
    "@yeoman/namespace": "^1.0.0",
    "chalk": "^5.3.0",
    "debug": "^4.1.1",
    "execa": "^8.0.1",
    "github-username": "^8.0.0",
    "json-schema": "^0.4.0",
    "latest-version": "^9.0.0",
    "lodash-es": "^4.17.21",
    "mem-fs-editor": "^11.0.1",
    "minimist": "^1.2.8",
    "read-package-up": "^11.0.0",
    "semver": "^7.5.4",
    "simple-git": "^3.20.0",
    "sort-keys": "^5.0.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.9",
    "@types/ejs": "^3.1.4",
    "@types/json-schema": "^7.0.15",
    "@types/minimist": "^1.2.3",
    "@types/semver": "^7.5.3",
    "@types/sinon": "^17.0.1",
    "@types/text-table": "^0.2.3",
    "@vitest/coverage-v8": "^2.1.8",
    "@yeoman/adapter": "^2.0.0",
    "@yeoman/eslint": "^0.2.0",
    "@yeoman/transform": "^2.0.0",
    "cpy-cli": "^5.0.0",
    "ejs": "^3.1.9",
    "inquirer": "^12.0.0",
    "jsdoc": "^4.0.2",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "sinon": "^19.0.0",
    "type-fest": "^4.26.1",
    "typescript": "^5.2.2",
    "vitest": "^2.1.8",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^4.4.1",
    "yeoman-test": "^10.0.1"
  },
  "peerDependencies": {
    "@yeoman/types": "^1.1.1",
    "mem-fs": "^4.0.0"
  },
  "engines": {
    "node": "^18.17.0 || >=20.5.0"
  },
  "overrides": {
    "yeoman-test": {
      "yeoman-generator": "file:."
    }
  }
}
