{
  "name": "yeoman-generator",
  "version": "0.12.2",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "generate"
  ],
  "homepage": "http://yeoman.io",
  "bugs": "https://github.com/yeoman/generator/issues",
  "author": "Chrome Developer Relations",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/yeoman/generator.git"
  },
  "scripts": {
    "test": "DEBUG=generators:* mocha test/*.js --reporter list --timeout 100000",
    "legacy": "DEBUG=generators:* mocha test/legacy.js --reporter dot --timeout 100000",
    "test-generator": "mocha test/generators/*.js --reporter spec --timeout 100000"
  },
  "dependencies": {
    "cheerio": "~0.12.0",
    "request": "~2.21.0",
    "rimraf": "~2.1.4",
    "tar": "~0.1.17",
    "diff": "~1.0.4",
    "mime": "~1.2.9",
    "underscore.string": "~2.3.1",
    "lodash": "~1.3.0",
    "mkdirp": "~0.3.5",
    "glob": "~3.2.0",
    "nopt": "~2.1.1",
    "cli-table": "~0.2.0",
    "debug": "~0.7.2",
    "isbinaryfile": "~0.1.8",
    "dargs": "~0.1.0",
    "async": "~0.2.8",
    "inquirer": "~0.2.0"
  },
  "devDependencies": {
    "mocha": "~1.11.0",
    "proxyquire": "~0.4.0"
  }
}
