{
  "name": "yeoman-generator",
  "version": "0.15.0-pre.3",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "license": "BSD",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "generate"
  ],
  "homepage": "http://yeoman.io",
  "bugs": "https://github.com/yeoman/generator/issues",
  "author": "Chrome Developer Relations",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/yeoman/generator.git"
  },
  "scripts": {
    "test": "istanbul cover _mocha --report lcovonly -- test/*.js --reporter list --timeout 100000 && cat ./coverage/lcov.info",
    "travis": "grunt --force",
    "legacy": "mocha test/legacy.js --reporter dot --timeout 100000",
    "test-generator": "mocha test/generators/*.js --reporter spec --timeout 100000",
    "blanket": {
      "pattern": [
        "lib",
        "main.js"
      ]
    }
  },
  "dependencies": {
    "cheerio": "~0.12.0",
    "rimraf": "~2.2.0",
    "diff": "~1.0.4",
    "mime": "~1.2.9",
    "underscore.string": "~2.3.1",
    "lodash": "~2.4.1",
    "mkdirp": "~0.3.5",
    "glob": "~3.2.0",
    "debug": "~0.7.2",
    "isbinaryfile": "~2.0.0",
    "dargs": "~0.1.0",
    "async": "~0.2.8",
    "inquirer": "~0.3.1",
    "iconv-lite": "~0.2.10",
    "shelljs": "~0.2.6",
    "findup-sync": "~0.1.2",
    "chalk": "~0.3.0",
    "text-table": "~0.2.0",
    "download": "~0.1.6",
    "request": "~2.29.0",
    "file-utils": "~0.1.1",
    "class-extend": "~0.1.0"
  },
  "devDependencies": {
    "mocha": "~1.15.1",
    "proxyquire": "~0.5.1",
    "sinon": "~1.7.3",
    "markdox": "~0.1.2",
    "benchmark": "~1.0.0",
    "grunt": "~0.4.2",
    "grunt-contrib-jshint": "~0.8.0",
    "grunt-mocha-test": "~0.8.1",
    "grunt-jscs-checker": "~0.3.0",
    "grunt-mocha-cov": "~0.1.1"
  }
}
