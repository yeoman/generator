{
  "name": "yeoman-generator",
  "version": "6.0.0-alpha.0",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "yeoman",
    "app"
  ],
  "homepage": "http://yeoman.io",
  "repository": "yeoman/generator",
  "license": "BSD-2-Clause",
  "author": "Yeoman",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.js",
      "default": "./dist/index.js"
    },
    "./typed": {
      "types": "./dist/index.d.js",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.js",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-types",
    "copy-types": "cpy \"src/**/*.d.(|c|m)ts\" dist/",
    "coverage": "c8 report --reporter=text-lcov | coveralls",
    "doc": "npm run doc:generate && npm run doc:fix && npm run doc:prettier",
    "doc:fix": "sed -i -e 's:^[[:space:]]*<!--[[:space:]]*$::g' -e 's:^[[:space:]]*-->[[:space:]]*$::g' $npm_package_config_doc_path$DOC_FOLDER/global.html || true",
    "doc:generate": "jsdoc -c jsdoc.json -d $npm_package_config_doc_path$DOC_FOLDER",
    "doc:prettier": "prettier $npm_package_config_doc_path$DOC_FOLDER --write --ignore-path .prettierignore-doc",
    "prepare": "npm run build",
    "pretest": "xo",
    "test": "c8 esmocha --forbid-only"
  },
  "config": {
    "doc_path": "../yeoman-generator-doc/"
  },
  "dependencies": {
    "@types/node": "^16.18.3",
    "@yeoman/namespace": "^0.0.1",
    "@yeoman/types": "^0.1.1",
    "chalk": "^5.1.2",
    "dargs": "^8.1.0",
    "debug": "^4.1.1",
    "execa": "^7.1.1",
    "github-username": "^7.0.0",
    "json-schema": "^0.4.0",
    "lodash": "^4.17.11",
    "mem-fs-editor": "^10.0.1",
    "minimist": "^1.2.8",
    "read-pkg-up": "^9.1.0",
    "semver": "^7.5.0",
    "simple-git": "^3.18.0",
    "sort-keys": "^5.0.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/lodash": "^4.14.194",
    "@types/semver": "^7.3.13",
    "@types/sinon": "^10.0.14",
    "c8": "^7.13.0",
    "cpy-cli": "^4.2.0",
    "esmocha": "^0.1.4",
    "inquirer": "^9.2.1",
    "jsdoc": "^4.0.2",
    "mem-fs": "^3.0.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "sinon": "^15.0.4",
    "through2": "^4.0.2",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^5.0.4",
    "xo": "^0.54.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^4.0.0-alpha.4",
    "yeoman-test": "^8.0.0-alpha.1"
  },
  "peerDependencies": {
    "yeoman-environment": "^4.0.0-alpha.0"
  },
  "peerDependenciesMeta": {
    "yeoman-environment": {
      "optional": true
    }
  },
  "engines": {
    "node": "^16.13.0 || >=18.12.0"
  }
}
