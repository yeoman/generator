{
  "env": {
    "amd": true,
    "mocha": true,
    "node": true
},
  "rules": {
    "camelcase": 0,
    "consistent-return": 0,
    "curly": 0,
    "eol-last":0,
    "eqeqeq": [2, "smart"],
    "handle-callback-err": 0,
    "no-alert": 2,
    "no-bitwise": 0,
    "new-cap": 2,
    "no-caller": 2,
    "no-console": 0,
    "no-debugger": 1,
    "no-dupe-keys": 2,
    "no-eq-null": 0,
    "no-extra-bind": 0,
    "no-extra-parens": 0,
    "no-mixed-spaces-and-tabs": [2, true],
    "no-new": 0,
    "no-path-concat": 0,
    "no-process-exit": 0,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unreachable": 1,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": 2,
    "quotes": [2, "single"],
    "strict": 2,
    "wrap-iife": [2, "any"],
    "yoda": 0
  }
}
